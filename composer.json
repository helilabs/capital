{
    "name": "helilabs/capital",
    "description": "Laravel package that provides set of classes to make use of common design patterns like (Repository Pattern, Factory Pattern).",
    "keywords": ["laravel", "Design Patterns","Repository Pattern","Factory Pattern","curd controller"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammed Manssour",
            "email": "manssour.mohammed@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Helilabs\\Capital\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "laravel/framework" : "~5"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "4.8.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Helilabs\\Capital\\Providers\\CapitalServiceProvider"
            ]
        }
    }
}
